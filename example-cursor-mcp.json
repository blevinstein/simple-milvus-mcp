{
  "mcpServers": {
    "simple-milvus-mcp": {
      "command": "node",
      "args": [
        "./src/index.js",
        "--host", "localhost",
        "--port", "19530",
        "--collection", "test_collection",
        "--embedding-model", "google/text-embedding-004"
      ],
      "env": {
        "GEMINI_API_KEY": "your-gemini-api-key"
      }
    },
    "simple-milvus-mcp-all": {
      "command": "node",
      "args": [
        "./src/index.js",
        "--host", "localhost",
        "--port", "19530",
        "--embedding-model", "google/text-embedding-004"
      ],
      "env": {
        "GEMINI_API_KEY": "your-gemini-api-key"
      }
    }
  }
}
